#include <iostream>
#include <string>
#include "UListaP.h"

using namespace std;
/*
Clase Conjunto
 Atributos
 Elem : Lista
 Metodos
 Crear()
booleano vacío ()
booleano pertenece (Elemento e)
inserta (Elemento e)
entero cardinal ()
entero ordinal (elemento e)
suprime (Elemento e)
Elemento muestrea ()
 fin definición clase
 */
 class ConjuntoL{
	private:
		ListaP elem;
	public:
		ConjuntoL();
		bool vacio();
		bool pertenece(int e);
		void inserta(int e);
		int cardinal();
		int ordinal(int e);
		void suprime(int e);
		int muestrea();
 };
#endif
